{
  "manifest_version": 3,
  "name": "标签页标题修改器",
  "version": "1.0",
  "description": "修改当前标签页的标题",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.svg",
      "48": "images/icon48.svg",
      "128": "images/icon128.svg"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}